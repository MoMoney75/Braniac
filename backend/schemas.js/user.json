{"$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "username",
        "password"
    ],
    "properties": {
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "minLength" : 8,
            "maxLength" : 25,
            "examples": [
                "Test_user$$"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "minLength" : 8,
            "maxLength" : 400,
            "examples": [
                "PASSword1234"
            ]
        }
    },
    "examples": [{
        "username": "Test_user$$",
        "password": "PASSword1234"
    }]
}
